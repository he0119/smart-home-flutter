{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        "schedule:monthly",
        "group:allNonMajor",
        ":semanticCommitTypeAll(chore)"
    ],
    "labels": [
        "dependencies"
    ],
    "rangeStrategy": "bump",
    "ignoreDeps": [
        "dart",
        "dev.flutter.flutter-plugin-loader:dev.flutter.flutter-plugin-loader.gradle.plugin"
    ]
}
