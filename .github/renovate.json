{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended",
        "schedule:monthly",
        "group:allNonMajor",
        ":semanticCommitTypeAll(chore)"
    ],
    "labels": [
        "dependencies"
    ],
    "rangeStrategy": "bump",
    "ignoreDeps": [
        "dart"
    ],
    "ignorePaths": [
        "**/android/**"
    ]
}
